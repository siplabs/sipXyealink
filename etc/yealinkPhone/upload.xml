        <setting name="Logo132x64.dob">
            <label>SIP-T2X LCD Logo (screensaver)</label>
            <type>
                <file>
                    <contentType>application/binary</contentType>
                    <rename>yealinkLogo132x64.dob</rename>
                    <moveTo>../</moveTo>
                </file>
            </type>
            <description>Logo in DOB format for SIP-T2X phones</description>
        </setting>
        <setting name="ringtones.zip">
            <label>Custom RingTones</label>
            <type>
                <file>
                    <contentType>application/zip</contentType>
                    <moveTo>RingTones/</moveTo>
                </file>
            </type>
            <description>ZIP archive with WAV files</description>
        </setting>
        <setting name="wallpapers.zip">
            <label>Wallpapers for colour LCDs</label>
            <type>
                <file>
                    <contentType>application/zip</contentType>
                    <moveTo>WallPapers/</moveTo>
                </file>
            </type>
            <description>ZIP archive with JPG and PNG files</description>
        </setting>
        <setting name="screensavers.zip">
            <label>Screensavers for colour LCDs</label>
            <type>
                <file>
                    <contentType>application/zip</contentType>
                    <moveTo>ScreenSavers/</moveTo>
                </file>
            </type>
            <description>ZIP archive with JPG and PNG files</description>
        </setting>
        <setting name="yealink_SIP-T38G_idle_screen.xml">
            <label>Idle screen XML</label>
            <type>
                <file>
                    <contentType>text/xml</contentType>
                    <rename>yealink_SIP-T38G_idle_screen.xml</rename>
                </file>
            </type>
            <description>IdleScreen.xml</description>
        </setting>
        <setting name="languages.zip">
            <label>Custom LCD Language correction files</label>
            <type>
                <file>
                    <contentType>application/zip</contentType>
                    <moveTo>Languages/</moveTo>
                </file>
            </type>
            <description>ZIP archive with UTF16LE text file, named lang+English.txt or lang-LangName.txt by default</description>
        </setting>
        <setting name="contacts.tar">
            <label>Local contacts</label>
            <type>
                <file>
                    <contentType>application/binary</contentType>
                    <moveTo>Contacts/</moveTo>
                    <rename>contacts.tar</rename>
                </file>
            </type>
            <description>TARball with PNG images in TARball and ContactData.xml.</description>
        </setting>
        <setting name="contacts.zip">
            <label>Custom Contacts sources files</label>
            <type>
                <file>
                    <contentType>application/zip</contentType>
                    <moveTo>Contacts/</moveTo>
                </file>
            </type>
            <description>ZIP archive with XML files: search.xml and favorites.xml</description>
        </setting>
        <setting name="customsoftkeys.zip">
            <label>Custom Softkeys Layout files</label>
            <type>
                <file>
                    <contentType>application/zip</contentType>
                    <moveTo>CustomSoftkeyLayout/</moveTo>
                </file>
            </type>
            <description>ZIP archive with XML file</description>
        </setting>
        <setting name="certs.zip">
            <label>SSL Certificates</label>
            <type>
                <file>
                    <contentType>application/zip</contentType>
                    <moveTo>Certs/</moveTo>
                </file>
            </type>
            <description>ZIP archive with SSL Certificates in PEM format</description>
        </setting>
</group>
</model>
